{
  "compilerOptions": {
    "target": "es2020",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "noEmit": true
  },
  "exclude": [
    "subgraph",
    "node_modules",
    "artifacts",
    "dependencies",
    "generated",
    "out",
    "build",
    "cache",
    "cache_forge"
  ]
}
